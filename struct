#include <stdio.h>
#include <string.h>
struct student{
    char name[20];
    int age;
    int cgpa;
    };
int main()
{
    struct student student1;
    strcpy(student1.name,"abc");
    student1.age=18;
    student1.cgpa=7;
    printf("details of student1: \n");
    printf("%s\n%d\n%d\n",student1.name,student1.age,student1.cgpa);
    
    struct student student2;
    strcpy(student2.name,"xyz");
    student2.age=18;
    student2.cgpa=6;
    printf("details of student2: \n");
    printf("%s\n%d\n%d\n",student2.name,student2.age,student2.cgpa);
    
    struct student student3;
    strcpy(student3.name,"efg");
    student3.age=18;
    student3.cgpa=8;
    printf("details of student3: \n");
    printf("%s\n%d\n%d",student3.name,student3.age,student3.cgpa);
}
